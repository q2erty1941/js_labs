function getGuestsCount(guestsInput){
    let firstNum = parseInt(guestsInput);
    if(isNaN(firstNum)){
        return 'not a number'
    }
    return firstNum;
}


console.log(getGuestsCount('Двоє'));
console.log(getGuestsCount('Я буду 1'));
